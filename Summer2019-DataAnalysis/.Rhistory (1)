axis(2,cex=1.2, at=c(0,5,10,15,20,25))
axis.POSIXct(1, cex=1.2, at=pretty(zoop[zoop$site_no=="BVR_50_p", "Hour",]))}
if(taxa.i==4){
mtext(paste(taxa[4]," Density",sep=""), side=4, line=2.5, cex=1)
axis(4,cex=1.2, at=pretty(zoop[zoop$site_no=="BVR_50_p", paste(taxa[taxa.i],"_density_NopL",sep="")]))
axis.POSIXct(1, cex=1.2, at=pretty(zoop[zoop$site_no=="BVR_50_p", "Hour",]))}
}
dev.off()
